extends layout

block content
  br
  div#repo.container
    table(width="100%")
      tbody
        each commit in commits
          tr
            td.author
              =commit.author.login
            td.sha
              =commit.short_sha
            td
              =commit.commit.message
            td
              a(href="/repos/"+repo.id+"/build/"+commit.sha)
                span.octicon.octicon-package